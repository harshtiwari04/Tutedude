#Task 1
def factorial(a):
    if a == 0 or a == 1:
        return 1
    else:
        return a * factorial(a - 1)

a = int(input("Enter a number: "))
print("Factorial of", a, "is", factorial(a))

#Task 2
import math

a=int(input("Enter a number"))
if(a>0):
    print("Square root : ",math.sqrt(a))
    print("Logarithmic : ",math.log(a))
    print("Sine : ",math.sin(a))
